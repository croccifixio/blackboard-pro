{
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Blackboard Pro",
        "path": "./themes/blackboard-pro.json",
        "uiTheme": "vs-dark"
      }
    ]
  },
  "description": "An extension of the default Blackboard theme including a themed workbench and more colorisation of snippets",
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/node": "^7.16.8",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-pipeline-operator": "^7.16.7",
    "cross-var": "^1.1.0",
    "dotenv": "^10.0.0",
    "dotenv-cli": "^4.1.1",
    "husky": "^6.0.0",
    "js-yaml": "^4.1.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "vsce": "^1.103.1"
  },
  "displayName": "Blackboard Pro",
  "engines": {
    "vscode": "^1.17.0"
  },
  "icon": "assets/blackboard-pro-dark.png",
  "license": "Unlicense",
  "name": "blackboard-pro",
  "publisher": "croccifixio",
  "repository": {
    "url": "https://github.com/croccifixio/blackboard-pro"
  },
  "scripts": {
    "build": "babel-node src/index.js",
    "deploy": "run-s deploy:*",
    "deploy:openvsx": "dotenv -- cross-var ovsx publish -p %OPENVSX_TOKEN%",
    "deploy:visualstudio": "vsce publish",
    "start": "nodemon",
    "prepare": "husky install",
    "test": "dotenv -- cross-var echo %HELLO%"
  },
  "version": "0.1.2"
}
